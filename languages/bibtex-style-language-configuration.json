{
	"comments": {
		"lineComment": "%"
	},
	"brackets": [
		["{", "}"]
	],
	"autoClosingPairs": [
		["{", "}"],
		["\"", "\""]
	],
	"surroundingPairs": [
		["{", "}"],
		["\"", "\""]
	],
	"wordPattern": "(#-?\\d+|[A-Za-z_.][0-9A-Za-z_.]*\\$?)",
	"indentationRules": {
		"increaseIndentPattern": "^((?!%).)*(\\{[^}\"]*)$",
		"decreaseIndentPattern": "^\\s*(\\}|if\\$|while\\$)"
	}
}
